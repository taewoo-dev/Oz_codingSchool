   "source": [
    "0부터 시작하여 n까지의 숫자 중에서 홀수만 생성하는 제너레이터 odd_numbers를 작성하세요."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 14,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[1, 3, 5, 7, 9]\n",
      "1\n",
      "3\n",
      "5\n",
      "7\n",
      "9\n"
     ]
    }
   ],
   "source": [
    "def odd_numbers(n : int):\n",
    "    for odd_number in range(1,n+1,2):\n",
    "        yield odd_number\n",
    "\n",
    "print(list(odd_numbers(9)))\n",
    "\n",
    "gen = odd_numbers(9)\n",
    "\n",
    "for i in range(5):\n",
    "    print(next(gen))\n",
    "\n"
   ]
  }
 ],
