   ],
   "source": [
    "def min_max(*args) -> None:\n",
    "    print(args)\n",
    "    print(type(args))\n",
    "    print(f\"최소값 : {min(args)}, 최대값 : {max(args)}\")\n",
    "\n",
    "min_max(23,4,5)"
   ]
  }
